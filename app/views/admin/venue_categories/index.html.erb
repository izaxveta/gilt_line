<% if @venue_categories.empty? %>
  <h3>There are currently no Venue Categories. Use the Venue Category Generator to update the system.</h3>
<% end %>
<%= link_to 'VENUE CATEGORY GENERATOR', venue_category_generator_path %>

<% @venue_categories.each do |venue_category| %>
  <div class='venue-category'>
    <%= link_to venue_category.name, venue_category_path(id: venue_category.id) %>
  </div>
<% end %>